select * from STG.CONFEC_TRACKER_LOAD_SP;

select * from STG.DATABASE_REFRESH_TRACKER where PRODUCT_CATEGORY='CONFECTIONARY';

UPDATE STG.DATABASE_REFRESH_TRACKER SET FINAL_FACT_REFRESH='Y'  where NON_ADDITIVE_INTRIM='Y';

UPDATE STG.DATABASE_REFRESH_TRACKER SET PRODUCT_STG='N',FACTDATA_STG='N',FCT_STG='N',MARKET_STG='N',PERIOD_STG='N',METADATA_STG='N'  
 where country_code Not in('PT','CH','UK','PL','NO','IT','SK') AND PRODUCT_CATEGORY='CONFECTIONARY';

